    <visual>
        <origin xyz="0 0 0" rpy="1.5708 0 0"/>
        <geometry>
            <cylinder radius="0.1" length="0.15"/>
        </geometry>
        <material name="black">
            <color rgba="0 0 0 1"/>
        </material>
    </visual>
</link>

<joint name="wheel_ml_joint" type="continuous">

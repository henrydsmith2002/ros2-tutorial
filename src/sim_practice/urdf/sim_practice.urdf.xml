<?xml version="1.0"?>
<!--<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="practice_rover">-->
<robot name="practice_rover">


<!--=========-->
<!--BASE_LINK_ROOT-->
<!--=========-->

<link name="base_link">
    <inertial>
        <mass value="5.0"/>
        <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
        <geometry>
            <box size="0.5 0.4 0.2"/>
        </geometry>
        <material name="blue">
            <color rgba="0 0 1 1"/>
        </material>
    </visual>
</link>

<!--==========-->
<!--WHEELS_SIX-->
<!--==========-->

<!--front_left-->
<link name="wheel_fl_link">
    <inertial>
        <mass value="0.5"/>
        <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
    <visual>
        <origin xyz="0 0 0" rpy="1.5708 0 0"/>
        <geometry>
            <cylinder radius="0.1" length="0.15"/>
        </geometry>
        <material name="black">
            <color rgba="0 0 0 1"/>
        </material>
    </visual>
</link>

<joint name="wheel_fl_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_fl_link"/>
    <origin xyz="0.35 0.3 -0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
</joint>

<!--front_right-->
<link name="wheel_fr_link">
    <inertial>
        <mass value="0.5"/>
        <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>

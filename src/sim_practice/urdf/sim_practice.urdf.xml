    <visual>
        <origin xyz="0 0 0" rpy="1.5708 0 0"/>
        <geometry>
            <cylinder radius="0.1" length="0.15"/>
        </geometry>
        <material name="black">
            <color rgba="0 0 0 1"/>
        </material>
    </visual>
</link>

<joint name="wheel_fr_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_fr_link"/>
    <origin xyz="0.35 -0.3 -0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
</joint>

